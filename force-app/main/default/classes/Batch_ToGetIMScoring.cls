global class Batch_ToGetIMScoring implements schedulable {
    
    global void execute(SchedulableContext ctx) {
        try{
        Date todayDt= Date.today();
        Date lastMonth = todayDt.addmonths(-1);
        System.debug(lastMonth.month());
        requestWrapper rw = new requestWrapper();
        rw.year = lastMonth.year();
        rw.month = lastMonth.month();
        List<IM_Scoring_Endpoints__c> listOfIMScoringEndpoints= IM_Scoring_Endpoints__c.getall().values();
        Http h1 = new Http();
        HttpRequest hreq = new HttpRequest();
        hreq.setEndPoint(listOfIMScoringEndpoints[0].End_Point__c);
        hreq.setMethod('GET');
        hreq.setHeader('Content-type','application/json');
        hreq.setHeader('Authorization', listOfIMScoringEndpoints[0].Token__c);
        hreq.setTimeout(30000);
        hreq.setBody(JSON.serialize(rw));
        HttpResponse res;
        if(!test.isRunningTest())
        res = h1.send(hreq);
        finalResponse listOfResWrapper = new finalResponse();
        listOfResWrapper = (finalResponse)JSON.deserialize(res.getBody(), finalResponse.class);
        System.debug('listOfResWrapper'+listOfResWrapper);
        List<responseWrapper> listOfReswrap = listOfResWrapper.data;
        List<ShowRoom_User__c> listOfShowRoomUSers= new List<ShowRoom_User__c>();
        for(responseWrapper rw1 : listOfReswrap){
            ShowRoom_User__c su = new ShowRoom_User__c();
            if(!String.ISBLANK(rw1.id)){
                su.Id = rw1.id;
                su.IM_Class__c = rw1.IM_Class;
                listOfShowRoomUSers.add(su);
            }
        }
        System.debug('listOfShowRoomUSers'+listOfShowRoomUSers);
        if(!listOfShowRoomUSers.isEmpty()){
            Database.update(listOfShowRoomUSers, false);
        }
        }Catch(Exception e){
            System.debug('Exception '+e.getMessage() + e.getLineNumber());
            Logger.log('Exception','Batch For Iron man Api','Batch_ToGetIMScoring class',e.getMessage(),e.getStackTraceString(),null);
        }
    }
    public class finalResponse{
        public String status;
        public List<responseWrapper> data;
    }
    public class responseWrapper{
        public String id;
        public String IM_Class;
    }
    public class requestWrapper{
        public Integer year;
        public Integer month;
    }
