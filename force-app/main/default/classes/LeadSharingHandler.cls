    private static LeadShare shareRecord(Lead l, Id UserOrGroupId) {
        LeadShare leadShr = new LeadShare();
        leadShr.LeadId = l.Id;
        leadShr.UserOrGroupId = UserOrGroupId;
        leadShr.LeadAccessLevel = 'Edit';
        leadShr.RowCause = Schema.LeadShare.RowCause.Manual;

        return LeadShr;
    }


    private static LeadShare shareRecordRead(Lead l, Id UserOrGroupId) {
        LeadShare leadShr = new LeadShare();
        leadShr.LeadId = l.Id;
        leadShr.UserOrGroupId = UserOrGroupId;
        leadShr.LeadAccessLevel = 'Read';
        leadShr.RowCause = Schema.LeadShare.RowCause.Manual;

        return LeadShr;
    }
