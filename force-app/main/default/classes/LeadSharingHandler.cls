    public static void shareReadWithManager(map < id, Lead > newlist, map < id, lead > oldlist) {
         List < LeadShare > leadShrLst = new List < LeadShare > ();
        for (Lead l: newlist.values()) {
            System.debug('---l.Square_Showroom_Manager__c---'+l.Square_Showroom_Manager__c);
            System.debug('---l.Square_Showroom_Agent__c---'+l.Square_Showroom_Agent__c);
                if(l.Square_Showroom_Manager__c != null && oldlist.get(l.Id).Square_Showroom_Manager__c != l.Square_Showroom_Manager__c){
                    leadShrLst.add(shareRecordRead(l, l.Square_Showroom_Manager__c));
                }
                if(l.Square_Showroom_Agent__c != null && oldlist.get(l.Id).Square_Showroom_Agent__c != l.Square_Showroom_Agent__c){
                    leadShrLst.add(shareRecordRead(l, l.Square_Showroom_Agent__c));
                }
                /*------ Roaster Change start---*/
                if(l.Design_Partner__c != null && l.Design_Partner__c != oldlist.get(l.Id).Design_Partner__c){ 
                    leadShrLst.add(shareRecordRead(l, l.Design_Partner__c));
                }
                /*------ Roaster Change end---*/
        }
      Database.SaveResult[] leadShareInsertResult = Database.insert(leadShrLst, false);
        
    }   

    public static void manualShareRead(map < id, Lead > newlist, map < id, lead > oldlist) {
        
                            lead l1 = new lead ();
        List < LeadShare > leadShrLst = new List < LeadShare > ();

        for (Lead l: newlist.values()) {
            if (oldlist == null) {
            system.debug('------------------oldlist-------------------->');
                if (l.ownerid == l.WT_Agent__c && l.Status == 'DP Assigned') {
                    system.debug('--------change dp first-----------');
                    //leadShrLst.add(shareRecord(l, l.Design_Partner__c));
