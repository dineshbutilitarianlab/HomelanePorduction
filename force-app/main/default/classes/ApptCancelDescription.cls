public class ApptCancelDescription {
    public static Boolean flag = false;
    public static void apptCancel(List<Lead> leadList, Map<Id,Lead> oldMap) {
        for(Lead l : leadList) {
            if(oldMap.get(l.Id).Status != l.Status && l.Status == 'Appointment Cancelled') {
                if(l.Description__c == null && oldMap.get(l.Id).Appointment_Time__c != null && flag == false) {
                    l.Description__c = '['+System.now().addHours(5).addMinutes(30)+'] - Appt at {'+oldMap.get(l.Id).Appointment_Time__c.addHours(5).addMinutes(30)+'} cancelled';
                    flag = true;
                }
                else if(l.Description__c != null && oldMap.get(l.Id).Appointment_Time__c != null && flag == false){
                    l.Description__c = '['+System.now().addHours(5).addMinutes(30)+'] - Appt at {'+oldMap.get(l.Id).Appointment_Time__c.addHours(5).addMinutes(30)+'} cancelled' +'\n'+l.Description__c;
                    flag = true;
                }
            }
        }
    }
