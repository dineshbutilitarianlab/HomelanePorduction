public class CallApiDownload{

    public String catchURL;
    public static Map<String,Object> firstLevelartifactsListMap  = new Map<String,Object>();
    public static Map<String,Object> secondLevelartifactsListMap = new Map<String,Object>();
    public static String S3URL;
    public String pName;
    public list<Element__c> lsteles;
    public API_Request_Response__c API_Request_Response{get;set;}
     
    public PageReference CallApiDownloadAction() {

        API_Request_Response= API_Request_Response__c.getInstance('GET');
        String eleId = ApexPages.currentPage().getParameters().get('elementId');
        lsteles = new list<Element__c>();
        lsteles = [SELECT Bucket__c,Bucket__r.Name,Description__c,Extension__c,File_Size__c,File_URL__c,Id,Name,OwnerId,
                Parent_Element__c,Parent_Element__r.name,Related_Object_ID__c,SystemModstamp,Type__c FROM Element__c
                Where id =: eleId];
                
                system.debug('lsteles ------------------------------>'+lsteles );

        HttpRequest req = new HttpRequest();
        HttpResponse res = new HttpResponse();
        Http http = new Http();

        if(lsteles[0].Parent_Element__r.name == NULL || lsteles[0].Parent_Element__r.name == ''){
            if(lsteles[0].Bucket__r.name == 'Site Measurement'){
                pName = 'measurements';
            } else if(lsteles[0].Bucket__r.name == '3D Drawings'){
                pName = '3d_files';
            } else if(lsteles[0].Bucket__r.name == '2D Drawings'){
                pName = '2d_drawings';
            } else {
                pName = 'others';
            }
        }else{
            if(lsteles[0].Parent_Element__r.name == 'Site Measurement'){
                pName = 'measurements';
            } else if(lsteles[0].Parent_Element__r.name == '3D Drawings'){
                pName = '3d_files';
            } else if(lsteles[0].Parent_Element__r.name == '2D Drawings'){
                pName = '2d_drawings';
            } else if(lsteles[0].Parent_Element__r.name == 'Quotation'){
                pName = 'quotations';
            } else {
                pName = 'others';
            }
        }
        

        list<opportunity> lstOpps = [Select id from opportunity where id=: lsteles[0].Related_Object_ID__c];
        
        System.debug('lstOppslstOppslstOpps : '+lstOpps[0].id);

       //String URL= 'https://artifactservice.homelane.com/v1/internals/customers/'+lstOpps[0].id+'/'+pName+'/';
       String URL= 'http://'+API_Request_Response.URL__c+'/v1/customers/'+lstOpps[0].id+'/artifacts/'+pName+'/';
       //system.debug('//////////////////'+URL);
       //system.debug('////////artifcatJson : //////////'+artifcatJson);
       String filename =  lsteles[0].name + lsteles[0].Extension__c;
       URL = URL + filename;

        req.setEndpoint(URL);
        req.setMethod('GET');
        req.setHeader('Content-Type', 'application/json'); 
        req.setHeader('x-hl-list-with-url', 'yes'); 
        req.setHeader('x-hl-expiry-sec', '3000'); 
        
        //req.setCompressed(true); // otherwise we hit a limit of 32000

     
        try {
        if(Test.isRunningTest()){
        //do something
        }else{
            res = http.send(req);
            System.debug(res.getBody());
            System.debug('RES JSON : '+res);
        }
            
        } catch(System.CalloutException e) {
          System.debug('Callout error: '+ e);
          System.debug(res.toString());
        }

        if(Test.isRunningTest()){
            S3URL = 'https://www.google.com';
        }else{
           
            if('' != res.getbody()){
                Map<String, Object> JSON_Input = (Map<String, Object>)JSON.deserializeUntyped(res.getbody());
                system.debug('*** JSON_Input '+JSON_Input.get('artifacts'));
                
                 firstLevelartifactsListMap = (Map<String, Object>)JSON_Input.get('artifacts');
                 system.debug('**** In firstLevelartifactsMap '+ firstLevelartifactsListMap );
        
                Decimal lastmodified ;
                Decimal FinalValue = null;
                List<Object> firstLevelartifactsList = (List<Object>)firstLevelartifactsListMap.get('artifact');
                 system.debug('**** In firstLevelartifactsList '+ firstLevelartifactsList );
                  for (Object item : firstLevelartifactsList) {
                         secondLevelartifactsListMap = (Map<String, Object>)item;
                            S3URL = (String)secondLevelartifactsListMap.get('url'); 
                       }
                 
                system.debug('*** S3URL is '+S3URL );
           }
                    
        }
        PageReference pageRef = new PageReference(S3URL);
        pageRef.setRedirect(true);
        return pageRef;
    }
    
    
    
    
