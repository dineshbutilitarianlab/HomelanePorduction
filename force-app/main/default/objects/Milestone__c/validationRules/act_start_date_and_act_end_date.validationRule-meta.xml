<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>act_start_date_and_act_end_date</fullName>
    <active>true</active>
    <errorConditionFormula>IF( Expected_Start_Date__c  &gt;  Expected_End_Date__c , true, false)</errorConditionFormula>
    <errorDisplayField>Expected_Start_Date__c</errorDisplayField>
    <errorMessage>Actual End Date should not be greater than Actual Start Date !!</errorMessage>
</ValidationRule>
