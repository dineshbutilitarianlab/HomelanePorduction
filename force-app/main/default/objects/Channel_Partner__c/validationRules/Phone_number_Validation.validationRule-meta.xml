<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_number_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>IF(AND( NOT(ISBLANK(Phone__c)),NOT(REGEX(Phone__c, &quot;\\D*?(\\d\\D*?){10}&quot;))), true, false)</errorConditionFormula>
    <errorMessage>Please input valid 10 digit contact number.</errorMessage>
</ValidationRule>
